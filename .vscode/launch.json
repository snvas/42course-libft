    "version": "0.2.0",
    "configurations": [
	{
		"type": "lldb",
		"request": "launch",
		"name": "CodeLLDB",
		"program": "${workspaceFolder}/src/libc/a.out",
		"args": [],
		"cwd": "${workspaceFolder}"
	},
        {
            "name": "gcc-9 - Criar e depurar o arquivo ativo",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/libc/a.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "sourceFileMap": {
                "/build/glibc-OTsEL5": "/usr/src/glibc"
            },
            "setupCommands": [
                {
                    "description": "Habilitar a reformatação automática para gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc-9 arquivo de build ativo",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}